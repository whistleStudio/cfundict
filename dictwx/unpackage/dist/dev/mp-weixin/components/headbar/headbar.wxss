.headbar {
  width: 100%;
  height: 110rpx;
  padding: 15rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #f0f0f0;
}
.headbar > view {
  width: 100%;
  height: 100%;
}
.headbar > view > view {
  height: 100%;
}
.headbar .default view {
  line-height: 70rpx;
  font-size: 40rpx;
  text-align: center;
  color: #323232;
}
.headbar .default view:first-of-type {
  width: 80rpx;
  margin-right: 20rpx;
  font-weight: bold;
}
.headbar .default view:nth-of-type(2) {
  width: 85rpx;
  height: 70rpx;
  background: url("data:image/png;base64,") center/contain no-repeat;
}
.headbar .default view:last-of-type {
  margin-left: auto;
  width: 70rpx;
  height: 70rpx;
  background-color: rgba(0, 0, 0, 0.2);
  border-radius: 50%;
}
.headbar .search view:first-of-type {
  width: 80%;
  position: relative;
}
.headbar .search view:first-of-type input {
  width: 100%;
  height: 70rpx;
  border: 2px solid #4dc7f1;
  margin-right: 10rpx;
  border-radius: 5rpx;
  padding: 0 65rpx 0 80rpx;
  box-sizing: border-box;
}
.headbar .search view:first-of-type text {
  background-color: white;
  z-index: 2;
  font-size: 40rpx;
  top: 50%;
  transform: translateY(-50%);
  position: absolute;
}
.headbar .search view:first-of-type text:first-of-type {
  color: #4dc7f1;
  left: 20rpx;
}
.headbar .search view:first-of-type text:last-of-type {
  color: #323232;
  right: 10rpx;
}
.headbar .search view:last-of-type {
  width: calc(20% - 30rpx - 10rpx);
  margin-left: auto;
  text-align: center;
  color: #4dc7f1;
  font: 33rpx/70rpx "微软雅黑", "Micosoft Yahei", "Micosoft YaHei", "sans-serif";
}